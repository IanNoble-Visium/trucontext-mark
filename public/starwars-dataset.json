  "storedQueries": [
    {
      "query": "MATCH (n:Character)-[r]->(m:Character) RETURN n,r,m",
      "lang": "cypher",
      "description": "Find all character relationships"
    },
    {
      "query": "MATCH (n:Character) WHERE n.forceUser = true RETURN n",
      "lang": "cypher",
      "description": "Find all Force users"
    },
    {
      "query": "MATCH (n:Character)-[r:BattledWith]->(m:Character) WHERE r.outcome = 'Victory' RETURN n,r,m",
      "lang": "cypher",
      "description": "Find all victorious battles"
    },
    {
      "query": "MATCH (n:Character) WHERE n.affiliation = 'Rebel Alliance' RETURN n",
      "lang": "cypher",
      "description": "Find all Rebel Alliance members"
    },
    {
      "query": "MATCH (n:Character)-[r:TrainedBy]->(m:Character) WHERE r.completed = true RETURN n,r,m",
      "lang": "cypher",
      "description": "Find completed training relationships"
    }
  ]
}